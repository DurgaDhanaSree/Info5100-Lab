<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <Label fx:id="nameLabel" layoutX="138.0" layoutY="112.0" prefHeight="18.0" prefWidth="56.0" text="Name:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="nameTextField" layoutX="199.0" layoutY="109.0" prefHeight="26.0" prefWidth="269.0" promptText="Name" />
      <Label fx:id="ageLabel" layoutX="148.0" layoutY="223.0" text="Age:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="ageTextField" layoutX="204.0" layoutY="220.0" prefHeight="26.0" prefWidth="158.0" promptText="Age" />
      <Label fx:id="emailLabel" layoutX="138.0" layoutY="170.0" text="Mail ID:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <TextField fx:id="emailTextField" layoutX="199.0" layoutY="167.0" prefHeight="26.0" prefWidth="245.0" promptText="Email ID" />
      <Button fx:id="submitButton" layoutX="255.0" layoutY="277.0" mnemonicParsing="false" onAction="#submit" text="Submit">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label layoutX="208.0" layoutY="55.0" prefHeight="20.0" prefWidth="150.0" text="REGISTRATION FORM">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
